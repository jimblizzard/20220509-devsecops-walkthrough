{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "subscriptionId": {
            "value": "<yourSubscriptionId>"
        },
        "rgName": {
            "value": "<prefix>webapps-rg"
        },
        "webAppName": {
            "value": "<prefix>webapps-web"
        },
        "location": {
            "value": "eastus"
        },
        "serverFarmName": {
            "value": "<prefix>webapps-asp"
        },
        "alwaysOn": {
            "value": true
        },
        "sku": {
            "value": "Standard"
        },
        "skuCode": {
            "value": "S1"
        },
        "workerSize": {
            "value": "0"
        },
        "workerSizeId": {
            "value": "0"
        },
        "numberOfWorkers": {
            "value": "1"
        },
        "linuxFxVersion": {
            "value": "DOTNETCORE|3.1"
        }
    }
}
